def majority_element_n_by_3(arr,self):
    if not arr:
        return []

    arr.sort()
    n = len(arr)
    result = []
    count = 1

    for i in range(1, n):
        if arr[i] == arr[i - 1]:
            count += 1
        else:
            if count > n // 3:
                result.append(arr[i - 1])
            count = 1

    # Check the last element
    if count > n // 3:
        result.append(arr[-1])

    return result
